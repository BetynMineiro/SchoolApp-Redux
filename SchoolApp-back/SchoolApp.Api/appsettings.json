{
  "SchoolApp": {
    "Auth0Config": {
      "Domain": "<<YOUR_AUTH0_DOMAIN_HERE>>", // e.g., "your-tenant.auth0.com"
      "Audience": "<<YOUR_AUTH0_AUDIENCE_HERE>>", // e.g., "https://your-api-identifier"
      "ClientId": "<<YOUR_AUTH0_CLIENT_ID_HERE>>",
      "ClientSecret": "<<YOUR_AUTH0_CLIENT_SECRET_HERE>>",
      "Connection": "<<YOUR_AUTH0_CONNECTION_NAME_HERE>>" // e.g., "Username-Password-Authentication"
    },
    "MongoConfig": {
      "Connection": "<<YOUR_MONGODB_CONNECTION_STRING_HERE>>", // e.g., "mongodb+srv://username:password@cluster.mongodb.net"
      "DataBase": "<<YOUR_MONGODB_DATABASE_NAME_HERE>>"
    }
  }
,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
